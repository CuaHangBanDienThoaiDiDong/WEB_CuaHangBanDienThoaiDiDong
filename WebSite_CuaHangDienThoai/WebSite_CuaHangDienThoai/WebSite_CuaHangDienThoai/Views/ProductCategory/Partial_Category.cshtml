@model IEnumerable<WebSite_CuaHangDienThoai.Models.tb_ProductCategory>
@if (Model != null)
{
    foreach (var item in Model)
    {
        <li><a href="/ProductCategory/Detail/@item.ProductCategoryId"  style="color:white;">@item.Title</a></li>
    }
}
else
{
    <p class="text-danger">Không có bảng ghi nào !!</p>
}