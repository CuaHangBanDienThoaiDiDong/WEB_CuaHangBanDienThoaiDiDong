<script>


     function LoadIndex(page) {
         $.ajax({
             url: '/Admin/Order/Partial_OrderIndex',
             type: 'GET',
             data: { page: page },
             success: function (rs) {
                 $('#loaddataOrderIndex').html(rs);
             },
             error: function (error) {
                 console.log(error);
             }
         });
     }
     $(document).ready(function () {
         originalContent = $('#loaddataOrderIndex').html();
         LoadIndex(1);

         $(document).on('click', '.pagination a', function (e) {
             e.preventDefault();
             var page = $(this).attr('data-page');
             LoadIndex(page);
         });
     });



</script>